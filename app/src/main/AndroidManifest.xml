<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dd.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Type_Cards"
            android:parentActivityName=".Type_ListItems" />
        <activity
            android:name=".Type_ListItems"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Place_Cards"
            android:parentActivityName=".Place_ListItems" />
        <activity
            android:name=".Place_ListItems"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Date_Cards"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>